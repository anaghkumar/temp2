pytest test_any.py -rA

# Run specific function
pytest test_any.py -rA -k login


# Run all function having login in name
pytest -rA -k login

# Getting Additional Info
pytest -rA -k login -v
pytest -rA -v


# Help
pytest -h

-k "String"
-v "verbose"

